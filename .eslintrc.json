{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "linebreak-style": "off"
    }
}
